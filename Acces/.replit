args = "PORT=3000 node RealisticHonorableDeskscan/server.js"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "PORT=5000 BLOCKCHAIN_PORT=5000 node RealisticHonorableDeskscan/blockchain-node.js"

